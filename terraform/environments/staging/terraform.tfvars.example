# Example Terraform variables for staging environment
# Copy this file to terraform.tfvars and fill in your values

# Kubernetes version (check available versions with: doctl kubernetes options versions)
kubernetes_version = "1.28.2-do.0"

# Cloudflare configuration
# Get account ID from Cloudflare dashboard -> Right sidebar
cloudflare_account_id = "your-cloudflare-account-id-here"

# Get zone ID from Cloudflare dashboard -> diocesan-vitality.org domain -> Overview
cloudflare_zone_id = "your-cloudflare-zone-id-here"
