# .env has secrets for tomknightatl/diocesan-vitality
# .env.example has starting template without secrets

# Supabase Secrets - Production (shared by default)
SUPABASE_KEY=<key>
SUPABASE_URL=<url>

# Supabase Secrets - Development (optional, for isolated dev database)
# Leave blank to use production database, or fill in to use separate dev project
SUPABASE_KEY_DEV=<key>
SUPABASE_URL_DEV=<url>

# AI Inference API Keys:
GENAI_API_KEY=<key>
SEARCH_API_KEY=<key>
SEARCH_CX=<key>

# Infrastructure & Cloud Services
DIGITALOCEAN_TOKEN=<key>
CLOUDFLARE_API_TOKEN=<key>
CLOUDFLARE_ACCOUNT_ID=<key>
CLOUDFLARE_ZONE_ID=<key>

# ArgoCD Admin Passwords (one per environment)
ARGOCD_ADMIN_PASSWORD_DEV=<secure_password_for_dev>
ARGOCD_ADMIN_PASSWORD_STG=<secure_password_for_staging>
ARGOCD_ADMIN_PASSWORD_PRD=<secure_password_for_production>
